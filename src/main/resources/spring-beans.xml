<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-4.2.xsd
        http://www.springframework.org/schema/context 
        http://www.springframework.org/schema/context/spring-context-4.2.xsd">
	<context:component-scan base-package="com.choice"></context:component-scan>
	<bean id="RZRQserviceImpl" class="com.choice.service.impl.RZRQserviceImpl"
		lazy-init="true" scope="prototype"></bean>
	<bean id="RzrqRequestHandServiceImpl" class="com.choice.service.impl.RzrqRequestHandServiceImpl" scope="prototype"></bean>
	<bean id="Rzrq" class="com.choice.model.Rzrq" scope="prototype"></bean>
	<bean id="RzrqKey" class="com.choice.model.RzrqKey" scope="prototype"></bean>
	<bean id="RzrqRequestHand" class="com.choice.model.RzrqRequestHand" scope="prototype"></bean>
</beans>        